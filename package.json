{
	"name": "wav-preview",
	"displayName": "wav-preview",
	"description": "preview and play wav file in VS Code",
	"version": "1.4.0",
	"engines": {
		"vscode": "^1.56.2"
	},
	"publisher": "sukumo28",
	"categories": [
		"Other"
	],
	"keywords": [
		"wav",
		"audio",
		"sound",
		"music",
		"viewer",
		"preview",
		"play",
		"listen"
	],
	"icon": "wav-preview.png",
	"repository": {
		"type": "git",
		"url": "https://github.com/sukumo28/wav-preview.git"
	},
	"bugs": {
		"url": "https://github.com/sukumo28/wav-preview.git"
	},
	"activationEvents": [
		"onCustomEditor:wavPreview.audioPreview"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"customEditors": [
			{
				"viewType": "wavPreview.audioPreview",
				"displayName": "Audio Preview",
				"selector": [
					{
						"filenamePattern": "*.wav"
					}
				],
				"priority": "default"
			}
		]
	},
	"capabilities": {
		"untrustedWorkspaces": {
			"supported": "limited",
			"description": "This ext does not play audio files in untrusted workspaces."
		}
	},
	"scripts": {
		"vscode:prepublish": "webpack --mode production",
		"webpack": "webpack --mode development",
		"webpack-dev": "webpack --mode development --watch",
		"test-compile": "tsc -p ./"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.0",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.56.0",
		"@typescript-eslint/eslint-plugin": "^4.1.1",
		"@typescript-eslint/parser": "^4.1.1",
		"eslint": "^7.9.0",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"ts-loader": "^8.0.15",
		"typescript": "^4.0.2",
		"vscode-test": "^1.4.0",
		"webpack": "^5.21.1",
		"webpack-cli": "^4.5.0"
	},
	"dependencies": {
		"wavefile": "^11.0.0"
	}
}
